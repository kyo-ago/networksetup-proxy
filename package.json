  },
  "dependencies": {
    "es6-promisify": "^5.0.0",
    "execa": "^0.8.0",
    "sudo-prompt": "^7.0.0"
  },
  "devDependencies": {
    "@types/execa": "^0.7.0",
    "@types/node": "^8.0.22",
    "fixpack": "^2.3.1",
    "typescript": "^2.2.1"
  },
  "homepage": "https://github.com/kyo-ago/networksetup-proxy/",
  "keywords": [
    "electron",
    "network",
    "networksetup",
    "os x",
