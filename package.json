{
  "name": "networksetup-proxy",
  "version": "0.0.5",
  "author": "@kyo_ago",
  "bugs": {
    "url": "https://github.com/kyo-ago/networksetup-proxy/issues"
  },
  "dependencies": {
    "electron-sudo": "^4.0.12",
    "execa": "^0.6.3"
  },
  "devDependencies": {
    "@types/node": "^7.0.11",
    "fixpack": "^2.3.1",
    "typescript": "^2.2.1"
  },
  "homepage": "https://github.com/kyo-ago/networksetup-proxy/",
  "keywords": [
    "electron",
    "network",
    "networksetup",
    "os x",
    "osx",
    "proxy"
  ],
  "license": "GPL-3.0",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kyo-ago/networksetup-proxy.git"
  },
  "scripts": {
    "build": "tsc -p ./tsconfig.json",
    "grant": "chmod 4755 ./rust/proxy-setting && sudo chown 0:0 ./rust/proxy-setting",
    "rust-build": "rustc -o ./rust/proxy-setting ./rust/proxy-setting.rs",
    "rust-install": "curl -sSf https://static.rust-lang.org/rustup.sh | sh",
    "pretest": "npm run build",
    "test": "(cd example && npm install && npm test)"
  }
